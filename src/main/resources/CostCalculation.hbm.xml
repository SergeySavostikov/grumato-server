<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.testEntity.CostCalculation" table="cost_calculation" schema="itcompany">
        <id name="orderCost" column="order_cost"/>
        <property name="orderCode" column="order_code"/>
        <property name="serviceCode" column="service_code"/>
        <property name="totalCost" column="total_cost"/>
    </class>
</hibernate-mapping>