<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.testEntity.EmployeeOrder" table="employee_order" schema="itcompany">
        <composite-id mapped="true" class="com.example.testEntity.EmployeeOrderPK">
            <key-property name="ordersOrderCode" column="orders_order_code"/>
            <key-property name="employeesEmployeeCode" column="employees_employee_code"/>
        </composite-id>
    </class>
</hibernate-mapping>