<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.entity.PriceListOfServicesEntity" table="price_list_of_services" schema="itcompany">
        <id name="serviceCode">
            <column name="service_code" sql-type="varchar(45)" length="45"/>
        </id>
        <property name="description">
            <column name="description" sql-type="varchar(200)" length="200"/>
        </property>
        <property name="цена">
            <column name="цена" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>