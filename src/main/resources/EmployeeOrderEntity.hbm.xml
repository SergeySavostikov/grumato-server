<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.entity.EmployeeOrderEntity" table="employee_order" schema="itcompany">
        <composite-id mapped="true" class="com.example.entity.EmployeeOrderEntityPK">
            <key-property name="ordersOrderCode">
                <column name="orders_order_code" sql-type="int(11)"/>
            </key-property>
            <key-property name="employeesEmployeeCode">
                <column name="employees_employee_code" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>