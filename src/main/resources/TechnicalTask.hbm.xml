<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.testEntity.TechnicalTask" table="technical_task" schema="itcompany">
        <id name="ttCode" column="tt_code"/>
        <property name="tkDescription" column="tk_description"/>
        <property name="cost" column="cost"/>
    </class>
</hibernate-mapping>